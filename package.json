{
  "name": "web-socket-lab",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon"
  },
  "engines": {
    "node": "22"
  },
  "nodemonConfig": {
    "ignore": [
      "**/node_modules/**",
      "**/docs/**",
      "**/test/**"
    ],
    "watch": [
      "server/src",
      "server/.env"
    ],
    "ext": "ts,tsx,js,jsx,html",
    "exec": "ts-node server/src/index.ts"
  },
  "devDependencies": {
    "loading-cli": "^1.1.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@parcel/config-default": "^2.13.3",
    "@parcel/core": "^2.13.3"
  }
}
